{% if errors == false %}
    {% if format_data == 'yml' %}
        <h2>{{ text_step_2_synchronization }}</h2>
        <p class="alert alert-success">{{ text_count_categories }}: <b>{{ count_categories }}</b>, {{ text_count_offers }}: <b>{{ count_offers }}</b></p>
        <table class="table table-bordered table-hover">
            <tbody>
            <tr>
                <td class="text-left">
                    {{ text_type_data_yml_setting_category_matching }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <div class="well" style="max-height: 200px; overflow-y: auto">
                            {% for yml_category_data_row in yml_categories %}
                                {% if template_data_selected.category.category_matching[yml_category_data_row.yml_category_id].select is defined and template_data_selected.category.category_matching[yml_category_data_row.yml_category_id]|length > 1 %}
                            <input class="setCategoryMatching" onchange="setCategoryMatching('{{ yml_category_data_row.yml_category_id }}',this.value,true)" name="import_yml_oc_template_data_yml.category.category_matching[{{ yml_category_data_row.yml_category_id }}].select" type="checkbox" id="yml_category_category_matching_{{ yml_category_data_row.yml_category_id }}" value="{{ yml_category_data_row.yml_category_id }}" checked="" />
                                {% else %}
                            <input class="setCategoryMatching" onchange="setCategoryMatching('{{ yml_category_data_row.yml_category_id }}',this.value,true)" name="import_yml_oc_template_data_yml.category.category_matching[{{ yml_category_data_row.yml_category_id }}].select" type="checkbox" id="yml_category_category_matching_{{ yml_category_data_row.yml_category_id }}" value="{{ yml_category_data_row.yml_category_id }}" />
                                {% endif %}
                                <label for="yml_category_category_matching_{{ yml_category_data_row.yml_category_id }}">&nbsp;&nbsp;&nbsp;{{ yml_category_data_row.name }}</label><br>
                                <div id="yml_category_category_matching_select{{ yml_category_data_row.yml_category_id }}" class="well" style="max-height: 200px; overflow-y: auto">
                                    <a onclick="setCategoryMatching('{{ yml_category_data_row.yml_category_id }}',this.value,true)">Редактировать список</a><br><br>
                                    {% set categories_matching_selected = [] %}
                                    {% set yml_category_id = yml_category_data_row.yml_category_id %}
                                    {% if template_data_selected.category.category_matching[yml_category_id].select is defined and template_data_selected.category.category_matching[yml_category_id].select and yml_categories and categories %}
                                        {% set template_data_selected = template_data_selected|merge({
                                            category: template_data_selected.category|merge({
                                                category_matching: template_data_selected.category.category_matching|merge({
                                                    yml_category_id: template_data_selected.category.category_matching[yml_category_id]|merge({
                                                        select: false
                                                    })
                                                })
                                            })
                                        }) %}
                                        {% if template_data_selected.category.category_matching[yml_category_id] %}
                                            {% set categories_matching_selected = template_data_selected.category.category_matching[yml_category_id] %}
                                        {% endif %}
                                    {% endif %}
                                    {% for category_id in categories_matching_selected %}
                                        {% if categories[category_id] %}
                                            <div id="setCategoryMatchingRow_{{ yml_category_id }}_{{ category_id }}__">
                                                <input checked="" onclick="removeSel({{ yml_category_id }},{{ category_id }},'_')" name="import_yml_oc_template_data_yml[category][category_matching][{{ yml_category_id }}][{{ category_id }}]" type="checkbox" value="{{ category_id }}" />
                                                <label>&nbsp;&nbsp;&nbsp;{{ categories[category_id].name }}</label><br>
                                            </div>
                                        {% endif %}
                                    {% endfor %}
                                </div>
                                <script>
                                    $(document).ready(function() {
                                        setCategoryMatching('{{ yml_category_data_row.yml_category_id }}',this.value,false);
                                    });
                                </script>
                            {% endfor %}
                        </div>
                    </div>
                </td>
            </tr>
            <tr>
                <td class="text-left">
                    {{ text_type_data_yml_setting_product_import_status }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <select onchange="openSettingBox('.product_import_status',this.value)" name="import_yml_oc_template_data_yml[product][import_status]" class="form-control">
                            {% if template_data_selected.product.import_status is defined and template_data_selected.product.import_status %}
                                <option value="1" selected="">{{ text_enabled }}</option>
                                <option value="0">{{ text_disabled }}</option>
                            {% else %}
                                <option value="0" selected="">{{ text_disabled }}</option>
                                <option value="1">{{ text_enabled }}</option>
                            {% endif %}
                        </select>
                    </div>
                    <script>
                        $(document).ready(function() {
                            $("select[name='import_yml_oc_template_data_yml[product][import_status]']").change();
                        });
                    </script>
                </td>
            </tr>
            <tr class="product_import_status">
                <td class="text-left">
                    {{ text_type_data_yml_setting_category_skip }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <div class="well" style="max-height: 200px; overflow-y: auto">
                            <a onclick="if($('.import_yml_oc_template_data_yml_product_category_skip').is(':checked')){ $('.import_yml_oc_template_data_yml_product_category_skip').prop('checked',false); }else{ $('.import_yml_oc_template_data_yml_product_category_skip').prop('checked',true); }">Выделение</a><br><br>
                            {% for yml_category_data_row in yml_categories %}
                                {% if template_data_selected.product.category_skip[yml_category_data_row.yml_category_id] %}
                                    <input class='import_yml_oc_template_data_yml_product_category_skip' name="import_yml_oc_template_data_yml[product][category_skip][{{ yml_category_data_row.yml_category_id }}]" type="checkbox" id="yml_category_skip_{{ yml_category_data_row.yml_category_id }}" value="{{ yml_category_data_row.yml_category_id }}" checked="" />
                                {% else %}
                                    <input class='import_yml_oc_template_data_yml_product_category_skip' name="import_yml_oc_template_data_yml[product][category_skip][{{ yml_category_data_row.yml_category_id }}]" type="checkbox" id="yml_category_skip_{{ yml_category_data_row.yml_category_id }}" value="{{ yml_category_data_row.yml_category_id }}" />
                                {% endif %}
                                <label for="yml_category_skip_{{ yml_category_data_row.yml_category_id }}">&nbsp;&nbsp;&nbsp;{{ yml_category_data_row.name }}</label><br>
                            {% endfor %}
                        </div>
                    </div>
                </td>
            </tr>
            <tr class="product_import_status">
                <td class="text-left">
                    {{ text_import_product_by_equal_value_attribute_or_field_value }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <select onchange="openSettingBox('.product_group_import',this.value)"  style="margin-bottom: 5px;" name="import_yml_oc_template_data_yml[product][group_import]" class="form-control">
                            {% if template_data_selected.product.group_import is defined and template_data_selected.product.group_import %}
                                <option value="1" selected="">{{ text_enabled }}</option>
                                <option value="0">{{ text_disabled }}</option>
                            {% else %}
                                <option value="0" selected="">{{ text_disabled }}</option>
                                <option value="1">{{ text_enabled }}</option>
                            {% endif %}
                        </select>
                        <div class="product_group_import">
                            <hr>
                            <br>
                            <label style="margin-top: 10px;">{{ text_products_with_same_value_chosen_attribute_add_like_one }}</label>
                            <select style="margin-bottom: 5px; max-width:150px;" id="group_import_attribute" name="import_yml_oc_template_data_yml[product][group_import_attribute]" class="form-control">
                                <option value="0">{{ text_type_data_yml_setting_no_import }}</option>
                                {% for offer_attribute_element, offer_attribute_element_name in offer_attributes %}
                                    {% if template_data_selected.product.group_import_attribute and template_data_selected.product.group_import_attribute and template_data_selected.product.group_import_attribute == offer_attribute_element %}
                                        <option value="{{ offer_attribute_element }}" selected="">{{ offer_attribute_element_name }}</option>
                                    {% else %}
                                        <option value="{{ offer_attribute_element }}">{{ offer_attribute_element_name }}</option>
                                    {% endif %}
                                {% endfor %}
                            </select>
                            <br>
                            <hr>
                            <label style="margin-top: 10px;">{{ text_products_with_same_value_chosen_field_yml_add_like_one }}</label>
                            <select style="margin-bottom: 5px; max-width:150px;" id="group_import_element" name="import_yml_oc_template_data_yml[product][group_import_element]" class="form-control">
                                <option value="0">{{ text_type_data_yml_setting_no_import }}</option>
                                {% for offer_element, tmp in offer_elements %}
                                    {% if template_data_selected.product.group_import_element is defined and template_data_selected.product.group_import_element and template_data_selected.product.group_import_element == offer_element %}
                                        <option value="{{ offer_element }}" selected="">{{ offer_element }}</option>
                                    {% else %}
                                        <option value="{{ offer_element }}">{{ offer_element }}</option>
                                    {% endif %}
                                {% endfor %}
                            </select>
                            <hr>
                            <label style="margin-top: 10px;">{{ text_how_to_create_whole_identificator }}</label>
                            <select style="margin-bottom: 5px; max-width:150px;" id="group_import_identificator" name="import_yml_oc_template_data_yml[product][group_import_identificator]" class="form-control">
                                {% if template_data_selected.product.group_import_identificator is defined and template_data_selected.product.group_import_identificator and template_data_selected.product.group_import_identificator == 'attribute' %}
                                    <option value="0">{{ text_pick_up }}</option>
                                    <option selected="" value="attribute">{{ text_make_product_id_from_specify_attribute }}</option>
                                    <option value="element">{{ text_make_product_id_from_specify_field_yml }}</option>
                                    <option value="attribute_element">{{ text_make_product_id_from_specify_attribute_and_field_yml }}</option>
                                {% elseif template_data_selected.product.group_import_identificator is defined and template_data_selected.product.group_import_identificator and template_data_selected.product.group_import_identificator == 'element' %}
                                    <option value="0">{{ text_pick_up }}</option>
                                    <option value="attribute">{{ text_make_product_id_from_specify_attribute }}</option>
                                    <option selected="" value="element">{{ text_make_product_id_from_specify_field_yml }}</option>
                                    <option value="attribute_element">{{ text_make_product_id_from_specify_attribute_and_field_yml }}</option>
                                {% elseif template_data_selected.product.group_import_identificator is defined and template_data_selected.product.group_import_identificator and template_data_selected.product.group_import_identificator == 'attribute_element' %}
                                    <option value="0">{{ text_pick_up }}</option>
                                    <option value="attribute">{{ text_make_product_id_from_specify_attribute }}</option>
                                    <option value="element">{{ text_make_product_id_from_specify_field_yml }}</option>
                                    <option selected="" value="attribute_element">{{ text_make_product_id_from_specify_attribute_and_field_yml }}</option>
                                {% else %}
                                    <option value="0">{{ text_pick_up }}</option>
                                    <option value="attribute">{{ text_make_product_id_from_specify_attribute }}</option>
                                    <option value="element">{{ text_make_product_id_from_specify_field_yml }}</option>
                                    <option value="attribute_element">{{ text_make_product_id_from_specify_attribute_and_field_yml }}</option>
                                {% endif %}
                            </select>
                            <hr>
                            <label>{{ text_make_prefix_for_product_from_group }}</label>
                            {% if template_data_selected.product.group_import_prefix %}
                                <input name="import_yml_oc_template_data_yml[product][group_import_prefix]" value="{{ template_data_selected.product.group_import_prefix }}" class="form-control" />
                            {% else %}
                                <input name="import_yml_oc_template_data_yml[product][group_import_prefix]" value="00000" class="form-control" />
                            {% endif %}
                            <hr>
                        </div>
                        <script type="text/javascript">
                            $(document).ready(function() {
                                $("select[name='import_yml_oc_template_data_yml[product][group_import]']").change();
                            });
                        </script>
                    </div>
                </td>
            </tr>
            <tr class="product_import_status">
                <td class="text-left">
                    {{ text_type_data_yml_setting_product_id_column }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <select name="import_yml_oc_template_data_yml[product][id_column]" class="form-control">
                            <option value="0">{{ entry_select }}</option>
                            {% for id_column, id_column_title in unique_types_data.product %}
                                {% if template_data_selected.product.id_column is defined and template_data_selected.product.id_column == id_column %}
                                    <option value="{{ id_column }}" selected="">{{ id_column_title }}</option>
                                {% else %}
                                    <option value="{{ id_column }}">{{ id_column_title }}</option>
                                {% endif %}
                            {% endfor %}
                        </select>
                    </div>
                </td>
            </tr>
            <tr class="product_import_status">
                <td class="text-left">
                    {{ text_type_data_yml_setting_product_status_default }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <select style="margin-bottom: 5px;" name="import_yml_oc_template_data_yml[product][status_default]" class="form-control">
                            {% if template_data_selected.product.status_default is defined and template_data_selected.product.status_default %}
                                <option value="1" selected="">{{ text_enabled }}</option>
                                <option value="0">{{ text_disabled }}</option>
                            {% else %}
                                <option value="0" selected="">{{ text_disabled }}</option>
                                <option value="1">{{ text_enabled }}</option>
                            {% endif %}
                        </select>
                    </div>
                </td>
            </tr>
            <tr class="product_import_status">
                <td class="text-left">
                    {{ text_type_data_yml_setting_product_price }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <select onchange="openSettingBox('.product_price_delta',this.value)" style="margin-bottom: 5px;" name="import_yml_oc_template_data_yml[product][price]" class="form-control">
                            {% if template_data_selected.product.price is defined and template_data_selected.product.price %}
                                <option value="1" selected="">{{ text_enabled }}</option>
                                <option value="0">{{ text_disabled }}</option>
                            {% else %}
                                <option value="0" selected="">{{ text_disabled }}</option>
                                <option value="1">{{ text_enabled }}</option>
                            {% endif %}
                        </select>
                        <div class="product_price_delta">
                            <label style="margin-top: 10px;">{{ text_type_data_yml_setting_product_price_delta }}</label>
                            {% if template_data_selected.product.price_delta is defined %}
                                <input placeholder="{{ text_type_data_yml_setting_product_price_delta }}" name="import_yml_oc_template_data_yml[product][price_delta]" value="{{ template_data_selected.product.price_delta }}" class="form-control" />
                            {% else %}
                                <input placeholder="{{ text_type_data_yml_setting_product_price_delta }}" name="import_yml_oc_template_data_yml[product][price_delta]" value="" class="form-control" />
                            {% endif %}
                            <br>
                            <label style="margin-top: 10px;">{{ text_type_data_yml_setting_product_price_param }}</label>
                            <table>
                                {% for i in range(low = 0, high = 8, step = 1) %}
                                    <tr>
                                        <td class="text-left">
                                            {{ text_type_data_yml_setting_product_price_param_limit }}
                                        </td>
                                        <td class="text-left">
                                            <div style="margin-left: 5px;" class="input-group">
                                                {% if template_data_selected.product.price_param[i].limit is defined %}
                                                    <input placeholder="{{ text_type_data_yml_setting_product_price_param_limit }}" name="import_yml_oc_template_data_yml[product][price_param][{{ i }}][limit]" value="{{ template_data_selected.product.price_param[i].limit }}" class="form-control" />
                                                {% else %}
                                                    <input placeholder="{{ text_type_data_yml_setting_product_price_param_limit }}" name="import_yml_oc_template_data_yml[product][price_param][{{ i }}][limit]" value="" class="form-control" />
                                                {% endif %}
                                            </div>
                                        </td>
                                        <td class="text-left" style="padding-left: 5px;">
                                            {{ text_type_data_yml_setting_product_price_param_delta }}
                                        </td>
                                        <td class="text-left">
                                            <div style="margin-left: 5px;" class="input-group">
                                                <div  style="margin-left: 5px;" class="input-group">
                                                    {% if template_data_selected.product.price_param[i].delta is defined %}
                                                        <input placeholder="{{ text_type_data_yml_setting_product_price_param_delta }}" name="import_yml_oc_template_data_yml[product][price_param][{{ i }}][delta]" value="{{ template_data_selected.product.price_param[i].delta }}" class="form-control" />
                                                    {% else %}
                                                        <input placeholder="{{ text_type_data_yml_setting_product_price_param_delta }}" name="import_yml_oc_template_data_yml[product][price_param][{{ i }}][delta]" value="" class="form-control" />
                                                    {% endif %}
                                                </div>
                                            </div>
                                        </td>
                                        <td class="text-left">
                                            <select name="import_yml_oc_template_data_yml[product][price_param][{{ i }}][delta_type]" class="form-control">
                                                {% if template_data_selected.product.price_param[i].delta_type is defined and template_data_selected.product.price_param[i].delta_type == 'currency' %}
                                                    <option value="currency" selected="">{{ text_cond_unit }}</option>
                                                    <option value="percent">%</option>
                                                {% else %}
                                                    <option value="currency">{{ text_cond_unit }}</option>
                                                    <option value="percent" selected="">%</option>
                                                {% endif %}
                                            </select>
                                        </td>
                                    </tr>
                                {% endfor %}
                            </table>
                            <br>
                            <label style="margin-top: 10px;">{{ text_type_data_yml_setting_product_price_special }}</label>
                            <select onchange="openSettingBox('.product_price_special_delta',this.value)" name="import_yml_oc_template_data_yml[product][price_special]" class="form-control">
                                {% if template_data_selected.product.price_special is defined and template_data_selected.product.price_special %}
                                    <option value="1" selected="">{{ text_type_data_yml_setting_product_price_special }}</option>
                                    <option value="0">{{ text_type_data_yml_setting_no_import }}</option>
                                {% else %}
                                    <option value="0" selected="">{{ text_type_data_yml_setting_no_import }}</option>
                                    <option value="1">{{ text_type_data_yml_setting_product_price_special }}</option>
                                {% endif %}
                            </select>
                        </div>
                        <script>
                            $(document).ready(function() {
                                $("select[name='import_yml_oc_template_data_yml[product][price]']").change();
                                $("select[name='import_yml_oc_template_data_yml[product][price_special]']").change();
                            });
                        </script>
                    </div>
                </td>
            </tr>
            <tr class="product_import_status">
                <td class="text-left">
                    {{ text_type_data_yml_setting_product_quantity }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <select onchange="openSettingBox('.product_quantity_param',this.value)" style="margin-bottom: 5px;" name="import_yml_oc_template_data_yml[product][quantity]" class="form-control">
                            {% if template_data_selected.product.quantity is defined and template_data_selected.product.quantity %}
                                <option value="1" selected="">{{ text_enabled }}</option>
                                <option value="0">{{ text_disabled }}</option>
                            {% else %}
                                <option value="0" selected="">{{ text_disabled }}</option>
                                <option value="1">{{ text_enabled }}</option>
                            {% endif %}
                        </select>
                        <div class="product_quantity_param">
                            <label style="margin-top: 10px;">{{ text_type_data_yml_setting_product_quantity_false_0 }}</label>
                            {% if template_data_selected.product.quantity_false_0 is defined %}
                                <input placeholder="{{ text_type_data_yml_setting_product_quantity_false_0 }}" name="import_yml_oc_template_data_yml[product][quantity_false_0]" value="{{ template_data_selected.product.quantity_false_0 }}" class="form-control" />
                            {% else %}
                                <input placeholder="{{ text_type_data_yml_setting_product_quantity_false_0 }}" name="import_yml_oc_template_data_yml[product][quantity_false_0]" value="0" class="form-control" />
                            {% endif %}
                            <hr>
                            <label style="margin-top: 10px;">{{ text_type_data_yml_setting_product_quantity_true_100 }}</label>
                            {% if template_data_selected.product.quantity_true_100 is defined %}
                                <input placeholder="{{ text_type_data_yml_setting_product_quantity_true_100 }}" name="import_yml_oc_template_data_yml[product][quantity_true_100]" value="{{ template_data_selected.product.quantity_true_100 }}" class="form-control" />
                            {% else %}
                                <input placeholder="{{ text_type_data_yml_setting_product_quantity_true_100 }}" name="import_yml_oc_template_data_yml[product][quantity_true_100]" value="100" class="form-control" />
                            {% endif %}
                            <br><label style="margin-top: 10px;">{{ text_type_data_yml_setting_product_quantity_false_status }}</label>
                            <select style="margin-bottom: 5px;" name="import_yml_oc_template_data_yml[product][quantity_false_status]" class="form-control">
                                {% if template_data_selected.product.quantity_false_status is defined and template_data_selected.product.quantity_false_status %}
                                    <option value="1" selected="">{{ text_enabled }}</option>
                                    <option value="0">{{ text_disabled }}</option>
                                {% else %}
                                    <option value="0" selected="">{{ text_disabled }}</option>
                                    <option value="1">{{ text_enabled }}</option>
                                {% endif %}
                            </select>
                        </div>
                        <script>
                            $(document).ready(function() {
                                $("select[name='import_yml_oc_template_data_yml[product][quantity]']").change();
                            });
                        </script>
                    </div>
                </td>
            </tr>
            <tr class="product_import_status">
                <td class="text-left">
                    {{ text_type_data_yml_setting_product_image }}
                </td>
                <td class="text-left">
                    <div class="input-group">
                        <select onchange="openSettingBox('.product_image_upload',this.value)"   style="margin-bottom: 5px;" name="import_yml_oc_template_data_yml[product][image]" class="form-control">
                            {% if template_data_selected.product.image is defined and template_data_selected.product.image %}
                                <option value="1" selected="">{{ text_enabled }}</option>
                                <option value="0">{{ text_disabled }}</option>
                            {% else %}
                                <option value="0" selected="">{{ text_disabled }}</option>
                                <option value="1">{{ text_enabled }}</option>
                            {% endif %}
                        </select>
                        <div class="product_image_upload">
                            <select style="margin-bottom: 5px;" name="import_yml_oc_template_data_yml[product][image_upload]" class="form-control">
                                {% if template_data_selected.product.image_upload is defined and template_data_selected.product.image_upload %}
                                    <option value="1" selected="">{{ text_type_data_yml_setting_product_image_upload_1 }}</option>
                                    <option value="0">{{ text_disabled }}</option>
                                {% else %}
                                    <option value="0" selected="">{{ text_type_data_yml_setting_product_image_upload_0 }}</option>
                                    <option value="1">{{ text_type_data_yml_setting_product_image_upload_1 }}</option>
                                {% endif %}
                            </select>
                        </div>
                        <div class="product_image_upload">
                            <select style="margin-bottom: 5px;" name="import_yml_oc_template_data_yml[product][image_new_name]" class="form-control">
                                {% if template_data_selected.product.image_new_name is defined and template_data_selected.product.image_new_name %}
                                    <option value="1" selected="">{{ text_keep_name_image }}</option>
                                    <option value="0">{{ text_create_new_name_image }}</option>
                                {% else %}
                                    <option value="0" selected="">{{ text_create_new_name_image }}</option>
                                    <option value="1">{{ text_keep_name_image }}</option>
                                {% endif %}
                            </select>
                        </div>
                        <div class="product_image_upload">
