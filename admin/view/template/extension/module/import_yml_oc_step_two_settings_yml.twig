    <script type="text/javascript">
        function selectUnselect(class_css) {
            if($(class_css).val()=="1") {
                $(class_css).val(0);
            }else{
                $(class_css).val(1);
            }

            return;
        }

        let column_name_num = 0;

        function addTypeDataColumnTableRow(type_process,template_id) {
            let id_td = new Date().getTime();
            let column_name = 'column_' + column_name_num;

            column_name_num += 1;

            let html = '';
            html += '<tr id="type_data_column_'+id_td+'_table_row">';
            html +=  '<td class="text-left" width="20%">';
            html +=    '<input onchange="setNewTypesDataColumns(\'' + column_name + '\',\'' + id_td + '\',this.value,\'' + type_process + '\',this.name,\'' + template_id + '\');" value="' + column_name + '" class="form-control" name="import_yml_oc_template_data_yml[export_field_name][' + template_id + '][' + column_name + ']">';
            html +=  '</td>';
            html +=  '<td class="text-left" width="20%">';
            html += 	  '<div class="input-group">';
            html += '<select onchange="getTypesDataColumns(this.value,\'#type_data_column_' + id_td + '\',\'' + column_name + '\',\'' + type_process + '\');" name="import_yml_oc_template_data_yml[type_data][' + column_name + ']" class="form-control select-type-data">';
            html += '<option value="0">{{ text_type_data_ignor }}</option>';

            {% for type, columns in types_data %}
            html += '<option value="{{ type }}">{{ "text_type_data_#{type}" }}</option>';
            {% endfor %}
            html +=          '</select>';
            html += 	  '</div>';
            html +=  '</td>';
            html +=  '<td class="text-left" id="type_data_column_' + id_td + '">';
            html +=  '</td>';
            html += '<td class="text-left"><button type="button" onclick="$(\'#type_data_column_' + id_td + '_table_row\').remove();" class="btn btn-danger"><i class="fa fa-minus-circle"></i></button></td>';
            html += '</tr>';

            $('#addTypeDataColumnTableRow_' + type_process).before(html);
        }
    </script>
    <script type="text/javascript">
        $(document).ready(function() {
            updateSaveButton();
        });
    </script>
